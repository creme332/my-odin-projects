{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-param-reassign": ["error", { "props": false }],
        "no-use-before-define": ["error", {
            "functions": false
        }],
        "no-underscore-dangle": "off",
        "no-plusplus":"off",
        "no-loop-func":"off",
        "no-restricted-syntax":"off",
        "no-promise-executor-return": "off",
        "import/extensions":"off"
    }
}